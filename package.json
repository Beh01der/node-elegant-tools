    "esbuild": "^0.18.0",
    "jest": "^29.5.0",
    "json-stringify-safe": "^5.0.1",
    "npm-dts": "^1.3.12",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
